<AnchorPane id="AnchorPane" prefHeight="629.0" prefWidth="879.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.FXMLControllers.Members.InsertMembersController">
    <stylesheets>
        <URL value="@insertmembers.css" />
    </stylesheets>
   <children>
      <GridPane layoutX="122.0" layoutY="181.0" prefHeight="268.0" prefWidth="636.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
               <children>
                  <Label text="Name" />
                  <TextField fx:id="tfName" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="2">
               <children>
                  <Label text="Surname" />
                  <TextField fx:id="tfSurname" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
               <children>
                  <Label text="Membership start date" />
                  <DatePicker fx:id="dpMembership" prefHeight="25.0" prefWidth="322.0" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Label text="Date of birth" />
                  <DatePicker fx:id="dpBirth" prefHeight="25.0" prefWidth="322.0" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <children>
                  <Label text="Phone number" />
                  <TextField fx:id="tfPhone" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="DNI" />
                  <TextField fx:id="tfDNI" />
               </children>
            </VBox>
         </children>
      </GridPane>
      <Button fx:id="btInsert" layoutX="197.0" layoutY="496.0" mnemonicParsing="false" onMouseClicked="#insertData" prefHeight="40.0" prefWidth="486.0" text="Add member" />
      <Label alignment="CENTER" layoutX="203.0" layoutY="68.0" prefHeight="47.0" prefWidth="474.0" text="ADD A MEMBER" />
      <Button fx:id="btMembers" layoutX="122.0" layoutY="68.0" mnemonicParsing="false" onMouseClicked="#goToMembers" />
   </children>
